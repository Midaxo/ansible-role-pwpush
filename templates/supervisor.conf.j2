[program:pwpush-internalweb-1]
command=bundle exec unicorn -p 5000 -c ./config/unicorn.rb
autostart=true
autorestart=true
stopsignal=QUIT
stdout_logfile=/var/log/pwpush/internalweb-1.log
stderr_logfile=/var/log/pwpush/internalweb-1.error.log
user={{pwpush_user}}
directory=/opt/PasswordPusher
environment=PORT="5000"
environment=RAILS_ENV=private

[group:pwpush]
programs=pwpush-internalweb-1
